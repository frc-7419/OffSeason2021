import frc.robot.subsystems.intake.RevolverSub;
import frc.robot.subsystems.sensors.RevColorDistanceSub;
import frc.robot.subsystems.shooter.*;

public class MoveRobot extends SequentialCommandGroup {

  public MoveRobot(DriveBaseSub driveBase) {
    addCommands(new StraightPowerTime(driveBase, 0.5, 1000));
    addCommands(new TurnPowerTime(driveBase, 0.5, TurnDirection.RIGHT, 1500));
    //this is a comment
  }
}
